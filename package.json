{
  "name": "mastermind_v2",
  "version": "1.0.0",
  "main": "backend/dist/index.js",
  "type": "module",
  "scripts": {
    "dev:backend": "ts-node-dev --respawn --transpile-only backend/src/index.ts",
    "build:backend": "tsc --project backend/tsconfig.json",
    "start:backend": "node backend/dist/index.js",
    "dev:frontend": "vite",
    "build:frontend": "tsc -b frontend/tsconfig.json && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview:frontend": "vite preview",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "start": "npm run start:backend",
    "preview": "npm run preview:frontend"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/material": "^6.1.8",
    "@radix-ui/react-slot": "^1.1.0",
    "argon2": "^0.41.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "lucide-react": "^0.461.0",
    "mongoose": "^8.8.1",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "react-toastify": "^10.0.6",
    "shadcn-ui": "^0.2.3",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/babel__core": "7.20.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.4.10"
  },
